{
  "name": "auto-publish-manage",
  "private": true,
  "version": "1.0.0",
  "description": "小白鸿蒙开源自助餐",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": " electron .",
    "electron:dev": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "electron:build": "vite build && electron-builder"
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.example.app",
    "productName": "MyElectronApp",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "build"
    }
  },
  "dependencies": {
    "axios": "^1.7.7",
    "element-plus": "^2.8.3",
    "qs": "^6.13.0",
    "vue": "^3.2.25",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "sass": "~1.32.6",
    "vite": "^2.9.5"
  }
}
